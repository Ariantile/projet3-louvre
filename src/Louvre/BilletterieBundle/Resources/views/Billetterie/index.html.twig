{% extends "LouvreBilletterieBundle:Billetterie:layout.html.twig" %}

{% block title %}
  Page d'accueil - {{ parent() }}
{% endblock %}

{% block header %}
<div id="headAccueil" class="container">
    <div id="ancreFormAcc" class="row">
        {% block form %}
        {% endblock %}
    </div>
</div>

{% endblock %}

{% block content %}

    {# Message flash #}
    {% for message in app.session.flashbag.get('info') %}
        <div class="alert alert-info">Message flash : {{ message }}</div>
    {% endfor %}
         
{% endblock %}